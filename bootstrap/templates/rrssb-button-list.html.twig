<div class="rrssb">
  {% if prefix_text %}
    <div class="rrssb-prefix">{{ prefix_text }}</div>
  {% endif %}
   


  <ul class="rrssb-buttons demo{{ i }}">
    {% for button in buttons %}
      <li class="rrssb-{{ button.name }} social-icon-{{loop.index}}">
        <a href="{{ button.link }}" {{ button.class | raw }}>
        </a>
      </li>
    {% endfor %}
  </ul>
</div>
